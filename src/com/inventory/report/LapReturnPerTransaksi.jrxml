	<queryString>
		<![CDATA[SELECT 
 br.no_return,
 br.no_referensi,
 br.tgl_return,
 br.`type`, 
 br.alasan,
 dbr.kode_barang,
 dbr.qty,
 b.nama_barang,
 b.satuan,
 b.harga
FROM barang_return br 
LEFT JOIN detail_barang_return dbr on br.no_return = dbr.no_return 
LEFT JOIN barang b on b.kode_barang = dbr.kode_barang 

WHERE br.no_return = $P{no}]]>

	<queryString>
		<![CDATA[SELECT
     pemesanan.`tgl_pesan` AS pemesanan_tgl_pesan,
     pemesanan.`total_pesan` AS pemesanan_total_pesan,
     distributor.`id_distributor` AS distributor_id_distributor,
     distributor.`nama_distributor` AS distributor_nama_distributor,
     pemesanan.`no_pesan` AS pemesanan_no_pesan
FROM
     `pemesanan` pemesanan INNER JOIN `distributor` distributor ON pemesanan.`id_distributor` = distributor.`id_distributor`
WHERE
    month(tgl_pesan) = $P{dt1} and year(tgl_pesan) = $P{dt2}]]>
	</queryString>
